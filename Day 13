#include <iostream>
using namespace std;
bool Pal(string &s, int low, int high) {
    while (low < high) {
        if (s[low] != s[high])
            return false;
        low++;
        high--;
    }
    return true;
}
string getLongestPal(string& s) {
   int n = s.size();
    int maxLen = 1, start = 0;
    for (int i = 0; i < n; i++) {
        for (int j = i; j < n; j++) {
            if (Pal(s, i, j) && (j - i + 1) > maxLen) {
                start = i;
                maxLen = j - i + 1;
            }
        }
    }

    return s.substr(start, maxLen);
}

int main() {
    string s = "babad";
    cout << getLongestPal(s) << endl;
    return 0;
}
