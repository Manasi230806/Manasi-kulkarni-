#include <bits/stdc++.h>
using namespace std;
vector<int> leaders(vector<int>& arr) {
    vector<int> r;
    int n = arr.size();
      for (int i = 0; i < n; i++) {
        int j;
        for (j = i + 1; j < n; j++) {
            if (arr[i] < arr[j])
                break;
        }
        if (j == n) 
            r.push_back(arr[i]);
    }
     return r;
}
int main() {
    vector<int> arr = { 16, 17, 4, 3, 5, 2 };
    vector<int> result = leaders(arr);
    for (int r : result) {
        cout << r << " ";
    }
    cout << endl;

    return 0;
}
