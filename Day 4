#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;
void mergeArrays(vector<int>& arr1, vector<int>& arr2) {
    int n = arr1.size();
    int m = arr2.size();
    vector<int> merged(n + m);
    for (int i = 0; i < n; ++i) {
        merged[i] = arr1[i];
    }
    for (int j = 0; j < m; ++j) {
        merged[n + j] = arr2[j];
    }
    sort(merged.begin(), merged.end());
    for (int i = 0; i < n; ++i) {
        arr1[i] = merged[i];
    }
    for (int j = 0; j < m; ++j) {
        arr2[j] = merged[n + j];
    }
}
int main() {
    vector<int> arr1 = {1, 3, 5, 7};
    vector<int> arr2 = {2, 4, 6, 8};

    mergeArrays(arr1, arr2);

    for (int num : arr1) {
        cout << num << ' ';
    }
    cout << endl;

    for (int num : arr2) {
        cout << num << ' ';
    }
    cout << endl;

    return 0;
}
